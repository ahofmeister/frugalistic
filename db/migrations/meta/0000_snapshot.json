  "views": {
    "public.transaction_auto_suggest": {
      "columns": {
        "unique_id": {
          "name": "unique_id",
          "type": "bigint",
          "primaryKey": false,
          "notNull": false
        },
        "description": {
          "name": "description",
          "type": "text",
          "primaryKey": false,
          "notNull": false
        },
        "type": {
          "name": "type",
          "type": "transaction_type",
          "typeSchema": "public",
          "primaryKey": false,
          "notNull": false
        },
        "category": {
          "name": "category",
          "type": "uuid",
          "primaryKey": false,
          "notNull": false
        },
        "name": {
          "name": "name",
          "type": "text",
          "primaryKey": false,
          "notNull": false
        },
        "color": {
          "name": "color",
          "type": "text",
          "primaryKey": false,
          "notNull": false
        },
        "frequency": {
          "name": "frequency",
          "type": "numeric",
          "primaryKey": false,
          "notNull": false
        }
      },
      "definition": "WITH category_counts\n                                                 AS (SELECT TRIM(BOTH FROM t_1.description) AS description,\n                                                            t_1.type,\n                                                            c.id                            AS category,\n                                                            c.name,\n                                                            c.color,\n                                                            count(*)                        AS frequency,\n                                                            row_number()                       OVER (PARTITION BY (TRIM(BOTH FROM t_1.description)), t_1.type ORDER BY (count(*)) DESC) AS rn\n                                                     FROM transactions t_1\n                                                              JOIN categories c ON c.id = t_1.category\n                                                     GROUP BY (TRIM(BOTH FROM t_1.description)), t_1.type, c.id,\n                                                              c.name, c.color),\n                                             totals AS (SELECT category_counts.description,\n                                                               category_counts.type,\n                                                               sum(category_counts.frequency) AS total_frequency\n                                                        FROM category_counts\n                                                        GROUP BY category_counts.description, category_counts.type)\n                                        SELECT row_number()         OVER (ORDER BY t.total_frequency DESC, cc.description) AS unique_id, cc.description,\n                                               cc.type,\n                                               cc.category,\n                                               cc.name,\n                                               cc.color,\n                                               t.total_frequency AS frequency\n                                        FROM category_counts cc\n                                                 JOIN totals t ON cc.description = t.description AND cc.type = t.type\n                                        WHERE cc.rn = 1\n                                        ORDER BY t.total_frequency DESC, cc.description",
      "name": "transaction_auto_suggest",
      "schema": "public",
      "isExisting": false,
      "with": {
        "securityInvoker": true
      },
      "materialized": false
    }
  },
